#include <stdio.h>
int main(){
for(int i=1;i<=10;i++){
printf("%d\n",i);}
return 0;}

ex1:#include <stdio.h>

int main()
{
for(int i=1;i<=50;i++){
printf("%d\n",i);}
return 0;}

ex2:#include <stdio.h>

int main()
{
for(int i=50;i>=1;i--){
printf("%d\n",i);}
return 0;}

ex3a:#include <stdio.h>

int main()
{
float nota1,nota2,media;
media=(nota1+nota2/2);
printf("coloque nota 1:\n);
scanf("d%",&nota1);
printf("coloque nota 2:\n);
scanf("f%",&nota2);
printf("sua media foi:f%",media);
return 0;}

ex3b:#include <stdio.h>

int main()
{
float nota1,nota2,media;
media=(nota1+nota2/2);
printf("coloque nota 1:\n");
scanf("f%",&nota1);
printf("coloque nota 2:\n");
scanf("f%",&nota2);
printf("sua media foi:f%",media);
if(media=>5){
printf("voce foi aprovado!");}
else if(media<5){
printf("voce foi reprovado');}
return 0;}

ex3c:#include <stdio.h>

int main()
{
int alunos,i,nota1,nota2;
float media;
printf("digite a quantidade de alunos a turma possui:\n");
scanf("%d",&alunos);
for(i=1;i<=alunos;i++){
printf("coloque nota1:\n");
scanf("%d",&nota1);
printf("coloque nota2:\n");
scanf("%d",&nota2);
media=((nota1+nota2)/2);
printf("sua media foi:%f\n",media);
if(media>=5){
printf("voce foi aprovado!\n");}
else if(media<5){
printf("voce foi reprovado\n");}
}
return 0;}

