#include <stdio.h>//exercicio 2 e 3
#include <stdlib.h>

int main()
{
int num;
printf("digite um numero:\n");
scanf("%d",&num);
printf("\n\t Imprimir os números de 50 até 1: \n");
while (num>=1)
{
printf(" %d\n",num);
num--;
}
return 0;
}

#include <stdio.h>//exercicio 4
#include <stdlib.h>

int main()
{
int num,i,resultado;
i=1;
printf("digite um numero para ver sua tabuada:\n");
scanf("%d",&num);
while (i<=10)
{
resultado =(num*i);
printf("%d x %d = %d\n",num,i,resultado);
i++;}
return 0;
}

